{
    "name": "thtg88/snake-cli",
    "description": "Play Snake in your terminal",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Marassi",
            "email": "thtg88@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "react/event-loop": "^1.3",
        "react/stream": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Thtg88\\SnakeCli\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
